{
  "buildCommand": "cd apps/miniapp && npm run build",
  "outputDirectory": "apps/miniapp/dist",
  "installCommand": "cd apps/miniapp && npm install",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
